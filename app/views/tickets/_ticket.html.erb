<div id="<%= dom_id ticket %>">
  <p>
    <strong>Title:</strong>
    <%= ticket.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= ticket.description %>
  </p>

  <p>
    <strong>Priority:</strong>
    <%= ticket.priority %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= ticket.category %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= ticket.status %>
  </p>

  <p>
    <strong>User ID:</strong>
    <%= ticket.user_id %>
  </p>

  <p>
    <strong>Project:</strong>
    <%= ticket.project_id %>
  </p>

</div>
